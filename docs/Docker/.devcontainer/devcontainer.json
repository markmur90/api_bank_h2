{
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": [
        "ms-python.python"
      ]
    }
  },
  // Removed duplicate "extensions" key as it is not allowed
  "mounts": [
    "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind",
    "source=../..,target=/app,type=bind,consistency=cached"
  ],
  // "remoteUser": "vscode",
  // "remoteEnv": {
  //   "PYTHONUNBUFFERED": "1"
  // },
  // Removed invalid "runArgs" property as it is not allowed
  "name": "swiftapi4",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "waitFor": "onCreateCommand",
  "updateContentCommand": "",
  "postCreateCommand": "apt install npm",
  "postAttachCommand": "",
  "containerEnv": {
    "DATABASE_URL": "postgres://postgres:postgres@db:5432/postgres"
  },
  // Removed duplicate "customizations" key
  "portsAttributes": {
    "8000": {
      "label": "Application"
    }
  },
  "forwardPorts": [8000]
}